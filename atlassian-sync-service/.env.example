# Example .env file for atlassian-sync-service
# Copy this file to .env and fill in your actual values

# General
NODE_ENV=development
PORT=3002
LOG_LEVEL=info

# API Keys (comma-separated for multiple keys)
VALID_API_KEYS=your-api-key-1,your-api-key-2

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Knowledge Graph Service
KNOWLEDGE_GRAPH_SERVICE_URL=http://localhost:4000

# Azure Service Bus
SERVICE_BUS_CONNECTION_STRING=your-service-bus-connection-string
SERVICE_BUS_TOPIC_NAME=your-topic-name
SERVICE_BUS_SUBSCRIPTION_NAME="kg-ingestion-worker"